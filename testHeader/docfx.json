{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "testHeader/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "testHeader/**"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.gif"
        ],
        "src": "../includes",
        "dest": "includes"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "searchScope": ["Azure"]
    },
    "fileMetadata": {},
    "template": [],
    "dest": "testHeader"
  }
}
